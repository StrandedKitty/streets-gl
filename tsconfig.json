{
	"compilerOptions": {
		"module": "es6",
		"target": "es6",
		"lib": [
			"es2019",
			"dom"
		],
		"allowJs": true,
		"sourceMap": true,
		"moduleResolution": "node",
		"alwaysStrict": true,
		"noImplicitAny": true,
		"noImplicitThis": true,
		"strictBindCallApply": true,
		"strictFunctionTypes": true,
		"resolveJsonModule": true,
		"allowSyntheticDefaultImports": true,
		"jsx": "react"
	},
	"include": [
		"src"
	]
}
